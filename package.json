{
  "name": "webRTC-demo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/start.js https=true",
    "build": "rm -rf ./output && rm -rf ./dist && node scripts/build.js && node ./config/publish.js",
    "test": "node scripts/test.js --env=jsdom port=8080"
  },
  "dependencies": {
    "@antv/g2": "^3.5.3",
    "@babel/helper-module-imports": "^7.0.0-beta.46",
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "6.26.0",
    "bignumber.js": "^8.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "^2.2.5",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "echarts": "^4.1.0",
    "es6-promise": "^4.2.4",
    "eslint": "4.18.2",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "eventemitter2": "^5.0.1",
    "eventemitter3": "^3.0.1",
    "fetch-polyfill": "^0.8.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "json-bigint": "^0.3.0",
    "object-assign": "4.1.1",
    "perfect-scrollbar": "^1.3.0",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "^8.0.1",
    "publish-helper": "git://github.com/daijinma/publish-helper.git#master",
    "raf": "3.4.0",
    "rc-form": "^2.1.7",
    "react": "^16.2.0",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.2.0",
    "react-lazyload": "^2.3.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-swipe": "^5.1.0",
    "react-tabs": "^2.1.1",
    "sa-sdk-javascript": "^1.12.14",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "antd": "^3.10.3",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-import": "^1.11.0",
    "babel-preset-es2015-ie": "^6.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "happypack": "^4.0.1",
    "html-loader": "^0.5.5",
    "less": "^3.0.0-alpha.3",
    "less-loader": "^4.1.0",
    "moment": "^2.22.2",
    "postcss-pxtorem": "^4.0.1",
    "react-router-dom": "^4.2.2",
    "superagent": "^3.8.3",
    "superagent-bluebird-promise": "^4.2.0",
    "uuid": "^3.3.2",
    "webpack-merge": "^4.1.2",
    "webpack-replace-loader": "^1.3.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "import/no-webpack-loader-syntax": 0
    }
  }
}
